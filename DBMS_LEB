CREATE TABLE ACCOUNT(
	ACTNO INT ,
	CNAME VARCHAR(100),
	BNAME VARCHAR(100),
	AMOUNT DECIMAL(8,2),
	ADATE DATE
)

SELECT * FROM [dbo].[ACCOUNT]


