7.=> db.DEPOSIT.findOne() // display only one 
8.=> db.DEPOSIT.insertOne({ACTON:109,CNAME:'KIRTI', BNAME:'VIRAR', AMOUNT:3000.00, ADATE:3-5-97})
9.=> db.DEPOSIT.insertMany([{ ACTON: 110,
  CNAME: 'MITALI',
